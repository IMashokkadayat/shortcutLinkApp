<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity"
        android:backgroundTint="#800082">

    <View
            android:layout_width="331dp"
            android:layout_height="348dp"
            android:id="@+id/view"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintVertical_bias="0.201" />

    <TextView
            android:hint="Expression"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:id="@+id/expression"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.104"
            app:layout_constraintVertical_bias="0.664" />

    <TextView
            android:hint="Agedata"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:id="@+id/agedata"
            app:layout_constraintStart_toEndOf="@+id/expression"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.141"
            app:layout_constraintVertical_bias="0.664" />

    <TextView
            android:hint="FaceShape"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:id="@+id/faceshape"
            app:layout_constraintStart_toEndOf="@+id/agedata"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.39"
            app:layout_constraintVertical_bias="0.664" />

    <TextView
            android:text="Expression"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/exptext"
            app:layout_constraintTop_toBottomOf="@+id/expression"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.122"
            app:layout_constraintVertical_bias="0.109" />

    <TextView
            android:text="Age"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            app:layout_constraintStart_toEndOf="@+id/exptext"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.21"
            app:layout_constraintVertical_bias="0.724" />

    <TextView
            android:text="Shape"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/facesp"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.447"
            app:layout_constraintVertical_bias="0.724" />


</androidx.constraintlayout.widget.ConstraintLayout>