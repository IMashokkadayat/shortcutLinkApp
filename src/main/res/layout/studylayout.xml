<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity"
        android:backgroundTint="#800080">

    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
              android:text="Study materials links avaliable here"
              android:textSize="20dp"
              android:textStyle="bold"
              app:layout_constraintLeft_toLeftOf="parent"
              app:layout_constraintRight_toRightOf="parent"
              app:layout_constraintTop_toTopOf="parent"
              app:layout_constraintBottom_toBottomOf="parent" tools:layout_editor_absoluteX="48dp"
              app:layout_constraintVertical_bias="0.096"/>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="TutorialPoint"
            android:backgroundTint="#808080"
            android:id="@+id/Tp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.289" app:layout_constraintHorizontal_bias="0.117"/>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="W3School"
            app:layout_constraintStart_toEndOf="@id/Tp"
            android:backgroundTint="#808080"
            android:id="@+id/ws"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="40dp"
            app:layout_constraintVertical_bias="0.278"/>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="GeeksForGeeks"
            android:id="@+id/GG"
            android:backgroundTint="#808080"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.43" app:layout_constraintHorizontal_bias="0.119"/>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="Programmiz"
            android:backgroundTint="#808080"
            android:id="@+id/Pr"
            app:layout_constraintStart_toEndOf="@id/GG"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="32dp"
            app:layout_constraintVertical_bias="0.448"/>

    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="CodeAcademy"
            android:id="@+id/ca"
            android:backgroundTint="#808080"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.587" app:layout_constraintHorizontal_bias="0.114"/>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="SimpleLearn"
            android:id="@+id/sl"
            android:backgroundTint="#808080"
            app:layout_constraintStart_toEndOf="@id/ca"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="44dp"
            app:layout_constraintVertical_bias="0.587"/>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:text="Guru99"
            android:id="@+id/Gg"
            android:backgroundTint="#808080"
            app:layout_constraintStart_toEndOf="@id/ca"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintVertical_bias="0.795"/>

</androidx.constraintlayout.widget.ConstraintLayout>
